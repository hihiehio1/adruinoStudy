        $.ajax({
            type: 'POST',
            dataType: 'json',
            url: '/eagle/receive',
            data: {
                'schematic': d_schematic,
                'board': d_board
            }
        }).success(function(data){
            window.location.href = "/eagle/crunch/?job="+data['job'];
        }).error(function(data){
            alert("Something went wrong, please try again!");
            location.reload();
        });

    }

