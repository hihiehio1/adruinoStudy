/* WEBPACK VAR INJECTION */(function($) {var Utils = function() {

};

Utils.getCSRFTokenCookie = function() {
    var csrfToken = $('meta[name="csrf-token"]').attr('content');
    if (!csrfToken) {
        document.cookie.split("; ").map(function (cookie) {
            var splitCookie = cookie.split('=');
            if (splitCookie[0] === "csrf-token")
                csrfToken = splitCookie[1];
        });
    }
    return csrfToken;
};

Utils.getCookie = function(key) {
    var data = document.cookie.split('; ').filter(x=>x.split('=')[0] == key);
    if (data.length) return data[0].split('=')[1];
};

Utils.setCookie = function(key, value) {
    var expiration = new Date();
    expiration.setTime(expiration.getTime() + 1000*60*60*24*365); // Expires in 1 year
    document.cookie = key + '=' + value + ";path=/;expires=" + expiration;
};

module.exports = Utils;
/* WEBPACK VAR INJECTION */}.call(exports, __webpack_require__(0)))

