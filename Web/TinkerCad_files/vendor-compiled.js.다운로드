			self.check_if_sticky();
		});

		self.check_if_sticky();
	}

	cSite.sitemenu.prototype.check_if_sticky = function(){
		if($('.js-main-sitemenu-is-sticky').length){
			$('.js-sitemenu').first().addClass('sitemenu--sticky');
		}else{
			$('.js-sitemenu').removeClass('sitemenu--sticky');
		}
	}

