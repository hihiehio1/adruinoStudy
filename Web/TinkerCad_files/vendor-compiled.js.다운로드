    cSite.analytics.clickTouch = function(element) {
        $element = $(element);
        data = $element.data() || {};
        properties = {};
        Object.keys(data).forEach(function(key) {
            if (key == 'event') {
                event = data[key];
            }
        });
        if (event) {
            cSite.analytics.log(event);
        }
    };

    cSite.analytics.log = function(event, item) {
        if (cSite.analytics.ga) {
            if (item) {
                cSite.analytics.ga('send', 'event', 'cir-ops', event, item);
            } else {
                cSite.analytics.ga('send', 'event', 'cir-ops', event);
            }
        }
    };
}( window['cSite'] = window['cSite'] || {}, jQuery ));

