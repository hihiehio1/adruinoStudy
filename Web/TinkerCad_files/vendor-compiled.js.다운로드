/* WEBPACK VAR INJECTION */(function(jQuery) {(function( cSite, $, undefined ){

	cSite.sitemenu_search = function() {
		var self = this;

		// EVENTS
		$('body').on('click', '.js-sitemenu__search__trigger', function(){
			self.do_search();
		});
		$('body').on('focusin', '.js-sitemenu__search__input', function(){
